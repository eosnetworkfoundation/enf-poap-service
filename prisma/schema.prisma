generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id          String @id @default(cuid())
  name        String
  description String
  imageUrl    String
  users       User[] @relation("UserToTokens")
}

model User {
  id      String  @id @default(cuid())
  address String  @unique
  tokens  Token[] @relation("UserToTokens")
}
